{
  "name": "Caltrain Countdown",
  "version": "1.1",
  "description": "Show the time remaining until the next caltrain arrives.",
  "manifest_version": 2,
  "browser_action": {
    "default_icon": "caltrain.png"
  },
  "icons": {
    "128": "caltrain.png"
  },
  "permissions": [
    "alarms",
    "storage"
  ],
  "background": {
    "scripts": ["caltrainNotifier.js"]
  }
}
